<div id="invite_head">  Invite People</div><div id ="invite_search">  <%= form_tag url_for(invite_articles_path), :remote => true do %>         <%= autocomplete_field_tag :user_id, params[:user_id], autocomplete_user_email_users_path, :update_elements => {}  %>         <%= hidden_field_tag :article_id, @article.id %>         <%= hidden_field_tag :status, 'false' %>         <%= submit_tag "Invite" %>       <% end %></div>